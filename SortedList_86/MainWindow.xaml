<Window x:Class="SortedList_86.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedList_86"
        mc:Ignorable="d"
        Title="MainWindow" Height="210.488" Width="390.571">
    <Grid>
        <TextBox x:Name="txt_key" HorizontalAlignment="Left" Height="23" Margin="29,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="233" TextChanged="txt_key_TextChanged"/>
        <TextBox x:Name="txt_value" HorizontalAlignment="Left" Height="23" Margin="29,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="233" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="btn_add" Content="Add" HorizontalAlignment="Left" Margin="274,65,0,0" VerticalAlignment="Top" Width="87" Height="83" Click="Button_add"/>
        <Label Content="Key" HorizontalAlignment="Left" Margin="29,39,0,0" VerticalAlignment="Top"/>
        <Label Content="Value" HorizontalAlignment="Left" Margin="29,99,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
